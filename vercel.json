{
  "version": 2,
  "builds": [
    {
      "src": "project_blog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLamdaSize": "15b",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_file.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project_blog/wsgi.py"
    }
  ]
}